  "regex": {
    "v_IP4": "\\b(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\b",
    "v_MAC": "([0-9A-F]{2}[\\.-]){5}([0-9A-F]{2})",
    "v_BaudN": "600|1200|2400|4800|9600|19200|38400|57600|115200|230400|460800|921600",
    "v_BaudC": "600|1200|2400|4800|9600|19200|38400|57600|115200|230400|460800|921600",
    "v_Cen_9x8": "^\\d{1,6}(\\.\\d{0,2})?$",
    "v_Num_0_1": "^[0-1]$",
    "v_Num_0_2": "^[0-2]$",
    "v_Num_0_4": "^[0-4]$",
    "v_Num_0_8": "^[0-8]$",
    "v_Num_0_9": "^[0-9]$",
    "v_Num_0_15": "^([0-9]|1[0-5])$",
    "v_Num_0_16": "^([0-9]|1[0-6])$",
    "v_Num_0_50": "^([0-9]|[1-4][0-9]|50)$",
    "v_Num_0_31": "^([0-9]|[1-2][0-9]|3[0-1])$",
    "v_Num_0_255": "^([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])$",
    "v_Num_0_999": "[0-9]{1,3}",
    "v_Num_20_90": "^([2-8][0-9]|90)$",
    "v_Num_9x14": "^[1-9][0-9]{0,13}$",
    "v_Prc_99_99": "^\\d{1,2}(\\.\\d{0,2})?$",
    "v_Pswd": "[0-9]{1,6}",
    "v_Qty_999999_999": "^\\d{1,6}(\\.\\d{0,3})?$",
    "v_Dummy": ".*"
  },
